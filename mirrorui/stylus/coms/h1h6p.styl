// Headings h1-6大标题
h1, h2, h3, h4, h5, h6
    font-family: inherit
    font-weight: 600
    line-height: 1.1
    color: inherit
h1, h2, h3, h4, h5, h6
    margin: .08rem auto
h1
    font-size: .4rem
h2
    font-size: .32rem
h3
    font-size: .28rem
h4
    font-size: .24rem
h5
    font-size: .2rem
h6
    font-size: .16rem
// p标记
p
    margin: 0 0 .14rem